local function addLogMessage(msg,author)local align=author=="User"and Enum.HorizontalAlignment.Right or Enum.HorizontalAlignment.Left;local color=author=="User"and Color3.fromRGB(40,40,60)or Color3.fromRGB(60,60,60);local txtColor=author=="User"and Color3.fromRGB(200,200,255)or Color3.fromRGB(220,220,220);local bubble=Instance.new("Frame");bubble.Size=UDim2.new(0.8,0,0,0);bubble.AutomaticSize=Enum.AutomaticSize.Y;bubble.BackgroundTransparency=1;bubble.Parent=chatLogContainer;local lyt=Instance.new("UIListLayout");lyt.HorizontalAlignment=align;lyt.Parent=bubble;local msgFrame=Instance.new("Frame");msgFrame.Size=UDim2.new(0,0,0,0);msgFrame.AutomaticSize=Enum.AutomaticSize.XY;msgFrame.BackgroundColor=color;msgFrame.Parent=bubble;Instance.new("UICorner",msgFrame).CornerRadius=UDim.new(0,8);local lbl=Instance.new("TextLabel");lbl.Text=msg;lbl.Font=Enum.Font.Code;lbl.TextSize=14;lbl.TextColor3=txtColor;lbl.BackgroundTransparency=1;lbl.TextWrapped=true;lbl.Size=UDim2.new(1,0,0,0);lbl.AutomaticSize=Enum.AutomaticSize.Y;lbl.Parent=msgFrame;Instance.new("UIPadding",msgFrame).Padding=UDim.new(0,8);local canvasY=chatLogContainer.UIListLayout.AbsoluteContentSize.Y;aiContent.CanvasSize=UDim2.new(0,0,0,canvasY);chatLogContainer.CanvasPosition=Vector2.new(0,canvasY)end
local function findPlayer(pN)if not pN then return nil end;local lN=pN:lower();for _,p in ipairs(Players:GetPlayers())do if p.Name:lower():find(lN,1,true)or p.DisplayName:lower():find(lN,1,true)then return p end end;return nil end
local function parseCommand(cT)addLogMessage(cT,"User");local a=cT:lower():split(" ");local c=a[1];local t,v;if #a>1 then t=findPlayer(a[2]);v=tonumber(a[2])end;local function r(m)addLogMessage(m,"AI")end;if c=="fly"then setFly(not isFlying);r("Voo "..(isFlying and"on"or"off"))elseif c=="speed"then if v then setSpeed(v);r("Speed: "..v)else r("Uso: speed [num]")end elseif c=="god"then local s=a[2]=="on";setGodMode(s);r("God "..(s and"on"or"off"))elseif c=="ff"then local s=a[2]=="on";setForcefield(s);r("FF "..(s and"on"or"off"))elseif c=="noclip"then setNoclip(not isNoclip);r("Noclip "..(isNoclip and"on"or"off"))elseif c=="esp"then setEsp(not isEspEnabled);r("ESP "..(isEspEnabled and"on"or"off"))elseif c=="aimbot"then setAimbot(not isAimbotEnabled);r("Aimbot "..(isAimbotEnabled and"on"or"off"))elseif c=="autofire"then isAutoFireEnabled=a[2]=="on";r("Autofire "..(isAutoFireEnabled and"on"or"off"))elseif c=="tp"then if t then teleportToPlayer(t);r("TP > "..t.Name)else r("Não encontrado")end elseif c=="puxar"then if t then pullPlayer(t);r("Puxando "..t.Name)else r("Não encontrado")end elseif c=="copiar"then if t then copySkin(t);r("Skin < "..t.Name)else r("Não encontrado")end elseif c=="telar"then if t then spectatePlayer(t);r("Telando "..t.Name)else r("Não encontrado")end elseif c=="stop"then spectatePlayer(nil,true);r("Parado.")elseif c=="help"then r("Comandos: fly, speed, god, ff, noclip, esp, aimbot, autofire, tp, puxar, copiar, telar, stop.")else r("Comando inválido.")end end
commandInput.FocusLost:Connect(function(e)if e and commandInput.Text~=""then parseCommand(commandInput.Text);commandInput.Text=""end end)
local function makeDraggable(tr,ta)local d=false;local s,p;tr.InputBegan:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then d=true;s=i.Position;p=ta.Position;local c;c=i.Changed:Connect(function()if i.UserInputState==Enum.UserInputState.End then d=false;c:Disconnect()end end)end end);UserInputService.InputChanged:Connect(function(i)if d and(i.UserInputType==Enum.UserInputType.MouseMovement or i.UserInputType==Enum.UserInputType.Touch)then local dl=i.Position-s;ta.Position=UDim2.new(p.X.Scale,p.X.Offset+dl.X,p.Y.Scale,p.Y.Offset+dl.Y)end end)end
makeDraggable(panelHeader,modPanel);makeDraggable(minimizedPanel,minimizedPanel);
