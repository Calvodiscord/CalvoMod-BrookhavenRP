local function switchTab(id)for _,c in pairs(contentArea:GetChildren())do if c:IsA("ScrollingFrame")then c.Visible=(c.Name==id)end end;for _,m in ipairs(menuButtons)do local s=(m.target==id);m.indicator.Visible=s;m.button.BackgroundTransparency=s and 0.8 or 1 end end
